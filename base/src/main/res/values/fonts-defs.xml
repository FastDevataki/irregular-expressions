<resources>


    <!--
    ========================================================
    Accent fonts use regular ASCII letters but use unicode
    characters that add accents of other decorations around
    them. Same accent will be applied to all letters.

    Character set fonts replace regular ASCII table printing
    characters with unicode characters to create different
    fonts; characters are defined for ASCII range:
    32 (space) - 126 (tilde).
    ========================================================
    -->

    <!-- follow this schema when defining accent character font! -->
    <!-- see existing arrays below for examples -->
    <integer name="AccentIdIndex">0</integer>
    <integer name="AccentNameIndex">1</integer>
    <integer name="AccentPremiumIndex">2</integer>
    <integer name="AccentValue">3</integer>

    <!-- follow this schema when defining font character sets! -->
    <!-- see existing arrays below for examples -->
    <integer name="CharsetIdIndex">0</integer>
    <integer name="CharsetNameIndex">1</integer>
    <integer name="CharsetPremiumIndex">2</integer>
    <integer name="CharsetReverseIndex">3</integer>
    <integer name="CharsetIndex">4</integer>

